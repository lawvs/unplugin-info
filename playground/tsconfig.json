{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "types": ["unplugin-info/client"],
    "useDefineForClassFields": true
  },
  "exclude": ["**/dist/**"],
  "include": ["./*.d.ts", "./*.ts"]
}
